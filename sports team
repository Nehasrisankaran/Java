import java.util.*;
public class Main{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int t;
        int l;
         t=a/b;
         l=a%b;
        System.out.println(t);
        System.out.println(l);
    }}
