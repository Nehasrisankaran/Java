package tervmec;
//LOWER BOUND-return index of target
import java.util.*;
public class Elite {
	public static void main(String[] args)
	{
		Scanner sc=new Scanner(System.in);
		int   a=sc.nextInt();
		int ar[]=new int[a];
		for(int i=0;i<a;i++) {
			ar[i]=sc.nextInt();
		}
		int target=sc.nextInt();
		int left=0,mid,store=0;
		int right=ar.length-1;
		if(target>ar[right]) {
			System.out.print(ar.length);
			return;
		}
		while(left<=right) {
			mid=left+right/2;
			if(ar[mid]>=target) {
				store=mid;
				right=mid-1;
			}
			else {
				left=mid+1;
			}
		}
		System.out.print(store);
	}
}
