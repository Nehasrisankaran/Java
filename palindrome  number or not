import java.util.*;
public class Main{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=a;
        int sum=0;
        while(a>0){
            int v=a%10;
            sum=sum*10+v;
            a/=10;
        }
        if(sum==b){
            System.out.print("Yes");
        }
        else{
            System.out.print("No");
        }
    }
}
