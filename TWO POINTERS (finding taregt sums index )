//TWO POINTERS
import java.util.*;
public class Elite {
	public static void main(String[] args)
	{
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		int ar[]=new int[a];
		for(int i=0;i<a;i++) {
			ar[i]=sc.nextInt();
		}
		int left=0;
		int right=ar.length-1;
		int target=sc.nextInt();
		while(left<right) {
			int sum=ar[left]+ar[right];
			if(sum==target) {
				System.out.print(left+" "+right);
				return;//using return does not execute any other code after getting ans but if we use break after breaking this loop it exits this loops and executes the below program and other loops and prints false every time unnecessarily
			}
			else if(sum>target) {
				right--;
			}
			else {
				left++;
			}
		}
		System.out.print("FALSE");}
}
