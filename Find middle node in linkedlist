		void display()
		{
			if(head==null)
			{
				System.out.println("Ennada onnumay illa");
				return;
			}
			Node temp=head;
			while(temp!=null)
			{
				System.out.println(temp.data);
				temp=temp.next;
			}
		}
		
		// slowpointer--fast pointer
		void middle() {
			Node slow = head;
			Node fast = head;
			while(fast!=null && fast.next!=null) {
				slow = slow.next;
				fast = fast.next.next;
			}
			System.out.print(slow.data);
		}
	}
	class Main{
		
		public static void main(String...d)
		{
		  LinkedList obj=new LinkedList(); 
		  obj.insert(10);
		  obj.insert(20);
		  obj.insert(30);
		  obj.insert(40);
		  obj.insert(50);
		  obj.insert(60);
		  obj.middle();
