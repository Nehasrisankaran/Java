		void insert(int d)
		{
			Node newnode=new Node(d);
			if(head==null)
			{
				head=newnode;
			}
			else {
				Node temp=head;
				while(temp.next!=null)
				{
					temp=temp.next;
				}
				temp.next=newnode;
			}
		}
//		void del() {
//			if(head==null)
//			{
