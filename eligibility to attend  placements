import java.util.*;
public class Main{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        String s=sc.nextLine();
        int r=sc.nextInt();
        float f=sc.nextFloat();
        int a=sc.nextInt();
        System.out.println(s);
        System.out.println(r);
        if(a==0&&f>=7.0||a<=2&&f>=7.5){
            System.out.print("Eligible to attend placement");
        }
        else{
            System.out.print("Not Eligible to attend placement");
        }
    }
}
