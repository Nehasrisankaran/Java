package NehaJava;
import java.util.*;
public class mec{
	public static void main(String args[])
	{
		Scanner sc =new Scanner(System.in);
		int a=sc.nextInt();
		//double b=sc.nextDouble();
		switch(a) {
		case 1:
			System.out.println("Yes");
			break;
		case 2:
			System.out.println("No");
			break;
			}
	}
}
