//MISSING NUMBER:
import java.util.*;
public class Elite {
	public static void main(String[] args)
	{
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		int ar[]=new int[a];
		for(int i=0;i<a;i++) {
			ar[i]=sc.nextInt();
		}
		Arrays.sort(ar);
        for(int i=0;i<ar.length-1;i++) {
        	if(ar[i]+1!=ar[i+1]) {
        		System.out.print(ar[i]+1);
        		break;
        	}
        }
	}}
			
