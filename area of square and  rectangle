import java.util.*;
public class Main{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
        double d=sc.nextDouble();
        double s=d*d;
        double e=(float)(3.14* s);
        System.out.println(a*a);
        System.out.println(b*c);
        System.out.printf("%.2f\n",e);
    }
}
