package NewPack;
import java.util.Scanner;

public class NewClass {

			public static void main (String[] args)
			{
		        Scanner read = new Scanner(System.in);
		        int N = read.nextInt();
		        for(int i=0;i<N;i++) {
		        	for(int j=0;j<=i;j++) {
		        		System.out.print("* ");
		        	}
		        	System.out.println();
		        }
		        read.close();	
		        }
}
